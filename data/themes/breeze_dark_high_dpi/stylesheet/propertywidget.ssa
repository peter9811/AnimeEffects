QSplitter#propertysplitter {
    width: 30ex;
	min-width: 20em;
}

QFrame#holder {
    background-color: #323232;
    border-radius: 0em;
}

QCheckBox#keyIcon {
}

QCheckBox#keyIcon::indicator {
    width: 1.2em;
    height: 1.2em;
}
QCheckBox#keyIcon::indicator:unchecked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_unchecked.svg);
}

QCheckBox#keyIcon::indicator:checked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_checked.svg);
}

QGroupBox#keyGroup {
    background-color: #323232;
    border:0em solid transparent;
    border-radius:0.4em;
    margin: 0.2em;
    padding-top: 1.8em;
    padding-bottom: 0.4em;
}

QGroupBox#keyGroup::title {
    subcontrol-origin: margin;
    subcontrol-position:top left;
	spacing: 1em;
    padding-left: 0em;
    padding-top: 0em;
	padding-bottom: 0.8em
}

QGroupBox#keyGroup::indicator {
    width: 1.2em;
    height: 1.2em;
    padding-left: 0.5em;
    padding-bottom: 0.2em;
}

QGroupBox#keyGroup::indicator:unchecked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_unchecked.svg);
}

QGroupBox#keyGroup::indicator:checked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_checked.svg);
}

QGroupBox#attrGroup {
    border-radius:0.4em;
    margin: 0.2em;
    padding-top: 1.6em;
    padding-bottom: 0.4em;
}

QGroupBox#attrGroup::title {
    subcontrol-origin: margin;
    subcontrol-position:top left;
	spacing: 1em;
    padding-left: 0em;
    padding-top: 0em;
	padding-bottom: 0.8em
}

QGroupBox#attrGroup::indicator {
    width: 1.2em;
    height: 1.2em;
    padding-left: 0.5em;
    padding-bottom: 0.2em;
}

QGroupBox#attrGroup::indicator:unchecked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_unchecked.svg);
}

QGroupBox#attrGroup::indicator:checked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_checked.svg);
}

QGroupBox#keyKnocker {
    background-color: #323232;
    border:0em solid transparent;
    border-radius:0.4em;
    margin: 0.2em;
    padding-top: 0.4em;
    padding-bottom: 0.4em;
}

QPushButton#keyKnocker {
    border: 0.1em solid #76797c;
    border-radius: 0.2em;
    padding: 0em;
    margin: 0em;
    width: 1em;
    height: 0.9em;
}
QPushButton#keyKnocker::enabled {
    image: url(data/themes/breeze_dark/icon/knockkey.png);
}
QPushButton#keyKnocker::disabled {
    image: url(data/themes/breeze_dark/icon/minus.png);
}

QPushButton#browser {
    border: 0.1em solid #76797c;
    border-radius: 0.2em;
    padding: 0.2em;
    width: 1.8em;
    height: 1.5em;
}
QPushButton#browser::enabled {
    image: url(data/themes/breeze_dark/icon/filew.png);
}
QPushButton#browser::disabled {
    image: url(data/themes/breeze_dark/icon/file.png);
}

QGroupBox#propertyPanel {
    background-color: #2d2d2d;
    border:0em solid transparent;
    border-radius:0.4em;
    margin: 0.2em;
    padding-top: 1.6em;
    padding-bottom: 0.4em;
}

QGroupBox#propertyPanel::title {
    subcontrol-origin: margin;
    subcontrol-position:top left;
	spacing: 1em;
    padding-left: 0em;
    padding-top: 0em;
	padding-bottom: 0.8em
}

QGroupBox#propertyPanel::indicator {
    width: 1.2em;
    height: 1.2em;
    padding-left: 0.5em;
    padding-bottom: 0.2em;
}

QGroupBox#propertyPanel::indicator:unchecked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_unchecked.svg);
}

QGroupBox#propertyPanel::indicator:checked {
    image: url(data/themes/breeze_dark/icon/breeze/checkbox_checked.svg);
}
